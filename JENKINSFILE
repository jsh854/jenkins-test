pipeline {
    agent any
    triggers{
        cron(BRANC_NAME=='testing' ? 'H/5 * 1-5 * *' : 'H/5 * * * *')
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}